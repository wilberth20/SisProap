<p id="notice"><%= notice %></p>

<h1>Listing Ciencia Tecnologia Avances</h1>

<table>
  <thead>
    <tr>
      <th>Nombre act</th>
      <th>Participantes</th>
      <th>Tipo act</th>
      <th>Inst nac internac</th>
      <th>Tipo partiplanif</th>
      <th>Fecha</th>
      <th>Informe avance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ciencia_tecnologia_avances.each do |ciencia_tecnologia_avance| %>
      <tr>
        <td><%= ciencia_tecnologia_avance.nombre_act %></td>
        <td><%= ciencia_tecnologia_avance.participantes %></td>
        <td><%= ciencia_tecnologia_avance.tipo_act %></td>
        <td><%= ciencia_tecnologia_avance.inst_nac_internac %></td>
        <td><%= ciencia_tecnologia_avance.tipo_partiplanif %></td>
        <td><%= ciencia_tecnologia_avance.fecha %></td>
        <td><%= ciencia_tecnologia_avance.informe_avance %></td>
        <td><%= link_to 'Show', ciencia_tecnologia_avance %></td>
        <td><%= link_to 'Edit', edit_ciencia_tecnologia_avance_path(ciencia_tecnologia_avance) %></td>
        <td><%= link_to 'Destroy', ciencia_tecnologia_avance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

