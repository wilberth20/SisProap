<p id="notice"><%= notice %></p>

<h1>Listing Presupuestos</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Descripcion</th>
      <th>Unidad</th>
      <th>Cantidad</th>
      <th>Costo unit</th>
      <th>Costo total</th>
      <th>Costo global</th>
      <th>Anexos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presupuestos.each do |presupuesto| %>
      <tr>
        <td><%= presupuesto.item %></td>
        <td><%= presupuesto.descripcion %></td>
        <td><%= presupuesto.unidad %></td>
        <td><%= presupuesto.cantidad %></td>
        <td><%= presupuesto.costo_unit %></td>
        <td><%= presupuesto.costo_total %></td>
        <td><%= presupuesto.costo_global %></td>
        <td><%= presupuesto.anexos %></td>
        <td><%= link_to 'Show', presupuesto %></td>
        <td><%= link_to 'Edit', edit_presupuesto_path(presupuesto) %></td>
        <td><%= link_to 'Destroy', presupuesto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Presupuesto', new_presupuesto_path %>
